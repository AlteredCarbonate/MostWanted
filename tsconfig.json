{
   "compilerOptions": {
      "typeRoots": ["./node_modules/@types", "./node_modules/@altv"],
      "target": "ES6",
      "removeComments": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowJs": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true
   }
}
